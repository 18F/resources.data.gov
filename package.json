{
  "name": "resources.data.gov",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "dependencies": {
    "uswds": "^2.0.3"
  },
  "devDependencies": {},
  "scripts": {
    "assets": "mkdir -p pages/assets/uswds; for dir in css fonts img js; do cp -r node_modules/uswds/dist/$dir pages/assets/uswds; done",
    "build": "npm run assets && bundle exec jekyll build",
    "clean": "rm -rf pages/assets/uswds _site",
    "federalist": "npm run assets",
    "setup": "bundle install",
    "start": "bundle exec jekyll serve",
    "test": "bundle exec htmlproofer --check-html --enforce-https _site"
  },
  "author": "Data.gov <datagovhelp@gsa.gov>",
  "license": "CC0-1.0"
}
